  "questions": [
    {
      "id": "q1",
      "type": "multiple-choice",
      "text": "Ich ___ Deutsch. (I know German)",
      "options": [
        {
          "id": "a",
          "text": "kenne"
        },
        {
          "id": "b",
          "text": "weiß"
        },
        {
          "id": "c",
          "text": "kann"
        },
        {
          "id": "d",
          "text": "kannte"
        }
      ],
      "correctAnswerId": "c",
      "explanation": "können = to know (a language/skill)",
      "explanationDetail": "Ich kann Deutsch (I know German / I can speak German). Use 'können' for languages and skills. 'Kennen' is for people/places, 'wissen' is for facts.",
      "tags": [
        "vocabulary",
        "common-mistakes",
        "quick-practice"
      ]
    },
    {
      "id": "q2",
      "type": "multiple-choice",
      "text": "Ich ___ Berlin. (I know Berlin)",
      "options": [
        {
          "id": "a",
          "text": "kenne"
        },
        {
          "id": "b",
          "text": "weiß"
        },
        {
          "id": "c",
          "text": "kann"
        },
        {
          "id": "d",
          "text": "kannte"
        }
      ],
      "correctAnswerId": "a",
      "explanation": "kennen = to know (people/places)",
      "explanationDetail": "Ich kenne Berlin (I know Berlin / I'm familiar with Berlin). Use 'kennen' for people, places, things. It means 'to be acquainted with'.",
      "tags": [
        "vocabulary",
        "common-mistakes",
        "quick-practice"
      ]
    },
    {
      "id": "q3",
      "type": "multiple-choice",
      "text": "Ich ___, dass Berlin groß ist. (I know that...)",
      "options": [
        {
          "id": "a",
          "text": "kenne"
        },
        {
          "id": "b",
          "text": "weiß"
        },
        {
          "id": "c",
          "text": "kann"
        },
        {
          "id": "d",
          "text": "kannte"
        }
      ],
      "correctAnswerId": "b",
      "explanation": "wissen = to know (facts)",
      "explanationDetail": "Ich weiß, dass Berlin groß ist (I know that Berlin is big). Use 'wissen' for facts and information. Often followed by 'dass' clause.",
      "tags": [
