  "questions": [
    { "id": "q1", "type": "multiple-choice", "text": "Ich stehe jeden Tag um 7 Uhr ___.", "options": [{ "id": "a", "text": "auf" }, { "id": "b", "text": "aufstehe" }, { "id": "c", "text": "stehe auf" }, { "id": "d", "text": "aufstehen" }], "correctAnswerId": "a", "explanation": "Separable prefix goes to end in present tense", "explanationDetail": "Aufstehen (to get up) is separable. In present tense, the prefix 'auf' goes to the end: Ich stehe... auf. The verb stem 'stehe' is conjugated and stays in position 2. So: Ich stehe auf (I get up).", "tags": ["verbs", "separable-verbs", "present-tense"] },
    { "id": "q2", "type": "multiple-choice", "text": "Er hat gestern ___.", "options": [{ "id": "a", "text": "angerufen" }, { "id": "b", "text": "anrufen" }, { "id": "c", "text": "ruft an" }, { "id": "d", "text": "anrief" }], "correctAnswerId": "a", "explanation": "Perfekt: prefix stays with past participle", "explanationDetail": "Anrufen (to call) in Perfekt: an + ge + rufen = angerufen. The prefix stays attached to the past participle. Er hat angerufen (he called/has called). Pattern: prefix + ge + stem + -en/-t.", "tags": ["verbs", "separable-verbs", "perfekt"] },
    { "id": "q3", "type": "multiple-choice", "text": "Wir ___ morgen früh ___.", "options": [{ "id": "a", "text": "fahren... ab" }, { "id": "b", "text": "abfahren" }, { "id": "c", "text": "fahren abfahren" }, { "id": "d", "text": "ab... fahren" }], "correctAnswerId": "a", "explanation": "Separable prefix goes to end", "explanationDetail": "Abfahren (to depart) is separable. In present tense: Wir fahren... ab (we depart/leave). The prefix 'ab' goes to the end of the clause. Wir fahren morgen ab (we're leaving tomorrow).", "tags": ["verbs", "separable-verbs", "present-tense"] },
    { "id": "q4", "type": "multiple-choice", "text": "Sie muss heute ___.", "options": [{ "id": "a", "text": "einkaufen" }, { "id": "b", "text": "kauft ein" }, { "id": "c", "text": "eingekauft" }, { "id": "d", "text": "ein kaufen" }], "correctAnswerId": "a", "explanation": "With modal verbs, separable verb stays as infinitive", "explanationDetail": "Einkaufen (to shop) with modal verb: the separable verb stays together as infinitive at the end. Sie muss einkaufen (she must shop). Modal verb is conjugated, infinitive goes to end as one word.", "tags": ["verbs", "separable-verbs", "modal-verbs"] },
    { "id": "q5", "type": "multiple-choice", "text": "Ich ___ die Tür ___.", "options": [{ "id": "a", "text": "mache... auf" }, { "id": "b", "text": "aufmache" }, { "id": "c", "text": "mache aufmachen" }, { "id": "d", "text": "auf... mache" }], "correctAnswerId": "a", "explanation": "Separable prefix goes to end", "explanationDetail": "Aufmachen (to open) is separable. In present tense: Ich mache... auf (I open). The prefix 'auf' separates and goes to the end. Ich mache die Tür auf (I open the door).", "tags": ["verbs", "separable-verbs", "present-tense"] },
    { "id": "q6", "type": "multiple-choice", "text": "Er ist gestern ___.", "options": [{ "id": "a", "text": "angekommen" }, { "id": "b", "text": "ankommen" }, { "id": "c", "text": "kommt an" }, { "id": "d", "text": "ankam" }], "correctAnswerId": "a", "explanation": "Perfekt: prefix stays with past participle", "explanationDetail": "Ankommen (to arrive) in Perfekt with sein: an + ge + kommen = angekommen. Er ist angekommen (he arrived/has arrived). Motion verbs use sein, and prefix stays attached.", "tags": ["verbs", "separable-verbs", "perfekt", "sein"] },
    { "id": "q7", "type": "multiple-choice", "text": "Du ___ das Licht ___.", "options": [{ "id": "a", "text": "machst... aus" }, { "id": "b", "text": "ausmachst" }, { "id": "c", "text": "machst ausmachen" }, { "id": "d", "text": "aus... machst" }], "correctAnswerId": "a", "explanation": "Separable prefix goes to end", "explanationDetail": "Ausmachen (to turn off) is separable. In present tense: Du machst... aus (you turn off). The prefix 'aus' goes to the end. Du machst das Licht aus (you turn off the light).", "tags": ["verbs", "separable-verbs", "present-tense"] },
    { "id": "q8", "type": "multiple-choice", "text": "Wir wollen heute ___.", "options": [{ "id": "a", "text": "ausgehen" }, { "id": "b", "text": "gehen aus" }, { "id": "c", "text": "ausgegangen" }, { "id": "d", "text": "aus gehen" }], "correctAnswerId": "a", "explanation": "With modal verbs, separable verb stays together", "explanationDetail": "Ausgehen (to go out) with modal verb: stays together as infinitive. Wir wollen ausgehen (we want to go out). The separable verb is not separated when used with modal verbs.", "tags": ["verbs", "separable-verbs", "modal-verbs"] },
    { "id": "q9", "type": "multiple-choice", "text": "Sie hat die Jacke ___.", "options": [{ "id": "a", "text": "angezogen" }, { "id": "b", "text": "anziehen" }, { "id": "c", "text": "zieht an" }, { "id": "d", "text": "anzog" }], "correctAnswerId": "a", "explanation": "Perfekt: prefix stays with past participle", "explanationDetail": "Anziehen (to put on) in Perfekt: an + ge + zogen = angezogen. Sie hat angezogen (she put on / has put on). Strong verb with vowel change: ziehen → gezogen.", "tags": ["verbs", "separable-verbs", "perfekt"] },
    { "id": "q10", "type": "multiple-choice", "text": "Ich ___ jeden Morgen ___.", "options": [{ "id": "a", "text": "räume... auf" }, { "id": "b", "text": "aufräume" }, { "id": "c", "text": "räume aufräumen" }, { "id": "d", "text": "auf... räume" }], "correctAnswerId": "a", "explanation": "Separable prefix goes to end", "explanationDetail": "Aufräumen (to clean up/tidy) is separable. In present tense: Ich räume... auf (I clean up). The prefix 'auf' goes to the end. Ich räume auf (I tidy up).", "tags": ["verbs", "separable-verbs", "present-tense"] },
    { "id": "q11", "type": "multiple-choice", "text": "Er kann nicht ___.", "options": [{ "id": "a", "text": "mitkommen" }, { "id": "b", "text": "kommt mit" }, { "id": "c", "text": "mitgekommen" }, { "id": "d", "text": "mit kommen" }], "correctAnswerId": "a", "explanation": "With modal verbs, separable verb stays together", "explanationDetail": "Mitkommen (to come along) with modal verb: stays together as infinitive. Er kann nicht mitkommen (he can't come along). Modal verb is conjugated, separable verb stays as one word at end.", "tags": ["verbs", "separable-verbs", "modal-verbs"] },
    { "id": "q12", "type": "multiple-choice", "text": "Sie ___ die Tür ___.", "options": [{ "id": "a", "text": "schließt... zu" }, { "id": "b", "text": "zuschließt" }, { "id": "c", "text": "schließt zuschließen" }, { "id": "d", "text": "zu... schließt" }], "correctAnswerId": "a", "explanation": "Separable prefix goes to end", "explanationDetail": "Zuschließen (to lock) is separable. In present tense: Sie schließt... zu (she locks). The prefix 'zu' goes to the end. Sie schließt die Tür zu (she locks the door).", "tags": ["verbs", "separable-verbs", "present-tense"] },
